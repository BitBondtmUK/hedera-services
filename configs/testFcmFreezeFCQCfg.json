{
  "name": "DynamicRestartFCQ",
  "description": "platform testing demo - FCM1K",
  "duration": 600,
  "settings": [
    {"name":"showInternalStats", "value": "1" },
    {"name":"csvFileName", "value":"PlatformTesting" },
    {"name":"csvAppend", "value":"true"},
    {
      "name": "state.saveStatePeriod",
      "value": "300"
    }
  ],
  "freezeConfig": {
    "freezeTiming": 2,
    "freezeIterations": 5,
    "postFreezeApp": {
      "jar": "PlatformTestingApp.jar",
      "parameterList": [
        "FCM1K.json"
      ]
    }
  },
  "experimentConfig": {
    "experimentRestartDelay": "5",
    "experimentStartDelay": "2",
    "freezeWaitMillis": "30000"
  },
  "useLifecycleModel" : true,
  "app": {
    "jar": "PlatformTestingApp.jar",
    "parameterList": [
      "FCM1KFreezeFCQ.json"
    ]
  },
  "resultFiles": [
    "swirlds.log"
  ],
  "memoryLeakCheckConfig": {
    "nodeGroupIdentifier": "FIRST"
  },
  "validator": [
    "STATS",
    "RESTART"
  ]
}