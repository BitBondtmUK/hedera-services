{
  "name":"DynamicRestart",
  "description" : "platform testing demo - FCM1K",
  "duration": 540,
  "settings": [
    {"name":"showInternalStats", "value": "1" },
    {"name":"csvFileName", "value":"PlatformTesting" },
    {"name":"csvAppend", "value":"true"}
  ],
  "freezeConfig": {
    "freezeTiming": 1,
    "freezeIterations": 5,
    "postFreezeApp": {
      "jar": "PlatformTestingApp.jar",
      "parameterList": ["FCM1K.json"]
    }
  },
  "app": {
    "jar": "PlatformTestingApp.jar",
    "parameterList": ["FCM1KFreeze.json"]
  },
  "resultFiles": [
    "swirlds.log"
  ],
  "validator": [ "STATS", "RESTART" ]
}
